{
	"compilerOptions": {
		"target": "es2016",
		"allowJs": false,
		"module": "commonjs",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"declaration": true,
		"resolveJsonModule": true,
		"rootDir": "./src",
		"outDir": "./dist",
		"typeRoots": ["src/api/types"]
	},
	"include": ["src"],
	"exclude": ["node_modules"]
}
